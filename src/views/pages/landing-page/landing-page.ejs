  <script>
    async function OpenFormDonate(id) {
      console.log('LOG-ASD', id);
      const hiddenInput = document.getElementById('item_id');
      hiddenInput.value = id;
    }

    async function SubmitDonate() {
      console.log('subcmit');
      try {
        const form = document.getElementById('formSubmitDonate');
        const formData = new FormData(form);

        const dataPayload = {};

        console.log('LOG-formData', formData);

        const fileInput = document.getElementById('file');

        formData.forEach((value, key) => {
          dataPayload[key] = value;
          console.log('LOG-foreachformData', key, value);
        });

        if (dataPayload.jumlah == '' || dataPayload.donatur == '' || dataPayload.bankAccount == '' || dataPayload.catatan == '' || fileInput.files.length == 0) {
          showToast('Form harus diisi semua!', 'info');
          return;
        }

        async function send() {
          const response = await fetch('/donasi/berdonasi', {
            method: 'POST',
            body: formData,
          });
        }
        send();

        showToast('Terimakasih sudah berdonasi, Donasi Terkirim!', 'success');
        const modal = bootstrap.Modal.getInstance(document.getElementById('donateNowModal'));
        modal.hide();

        document.getElementById('formSubmitDonate').reset();
        return;
      } catch (error) {
        console.error('LOG-ERROR', error);
        showToast('Donasi Gagal Terkirim!', 'error');
      }
    }

    function showToast(message, type = 'default') {
      const container = document.getElementById('toastContainer');

      // Create toast element
      const toast = document.createElement('div');
      toast.className = `toast ${type}`;
      toast.textContent = message;

      // Add toast to container
      container.appendChild(toast);

      // Trigger reflow and add show class
      setTimeout(() => {
        toast.classList.add('show');
      }, 10);

      // Remove toast after 3 seconds
      setTimeout(() => {
        toast.classList.remove('show');
        setTimeout(() => {
          container.removeChild(toast);
        }, 300);
      }, 3000);
    }
  </script>
